{
  "name": "serverless-plugin-browserifier",
  "version": "0.0.0-semantic",
  "description": "Serverless v1 plugin that uses browserify to bundle Node.js lambda functions",
  "main": "index.js",
  "scripts": {
    "format": "./node_modules/.bin/eslint --fix --ext .js .",
    "lint": "./node_modules/.bin/eslint --ext .js .",
    "pretest": "npm run lint",
    "test": "echo \"no tests yet =(\"",
    "release": "semantic-release"
  },
  "dependencies": {
    "archiver": "^1.3.0",
    "bluebird": "^3.7.2",
    "browserify": "^16.5.0",
    "filesize": "^3.6.1",
    "fs-extra": "^0.30.0",
    "globby": "^6.1.0",
    "lodash": "^4.17.15",
    "semver": "^5.7.1"
  },
  "peerDependencies": {
    "browserify": ">=13.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/git": "7.0.17",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "husky": "4.2.1",
    "lint-staged": "10.0.7",
    "prettier": "1.14.2",
    "semantic-release": "15.13.28",
    "standard": "11.0.1"
  },
  "author": "Ricardo Nolde <ricardo@nolde.com.br>",
  "license": "MIT",
  "homepage": "https://github.com/digitalmaas/serverless-plugin-browserifier#readme",
  "bugs": {
    "url": "https://github.com/digitalmaas/serverless-plugin-browserifier/issues"
  },
  "contributors": [
    "Ryan Pendergast <ryan.pendergast@gmail.com> (http://rynop.com)",
    "Ricardo Nolde <ricardo@nolde.com.br>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalmaas/serverless-plugin-browserifier.git"
  },
  "engines": {
    "node": ">=4.3"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "digitalmaas",
    "serverless",
    "serverless-plugin",
    "plugin",
    "browserify",
    "browserify-transform",
    "browserify-tool",
    "bundle",
    "lambda",
    "aws",
    "aws-lambda",
    "node.js",
    "node"
  ]
}
